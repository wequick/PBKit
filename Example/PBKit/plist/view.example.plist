			<key>clazz</key>
			<string>PBButton</string>
			<key>properties</key>
			<dict>
				<key>text</key>
				<string>Down</string>
				<key>!click</key>
				<dict>
					<key>type</key>
					<string>animate</string>
					<key>anims</key>
					<array>
						<dict>
							<key>type</key>
							<string>constraint</string>
							<key>target</key>
							<string>@title</string>
							<key>keyPath</key>
							<string>height</string>
