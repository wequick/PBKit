			<dict>
				<key>text</key>
				<string>Pbind</string>
				<key>textColor</key>
				<string>#fff</string>
				<key>textAlignment</key>
				<string>:center</string>
				<key>backgroundColor</key>
				<string>#f00@.8</string>
				<key>!click</key>
				<dict>
					<key>type</key>
					<string>alert</string>
					<key>title</key>
					<string>Hello</string>
				</dict>
			</dict>
		</dict>
		<key>tips</key>
		<dict>
			<key>clazz</key>
