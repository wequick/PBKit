	<array>
		<string>|-20-[down(==60)]-[up(==60)]</string>
		<string>V:[down(==44)]-20-[title(==30)]-20-[tips]</string>
		<string>|-20-[title]</string>
	</array>
	<key>constraints</key>
	<array>
		<string>T:down=20</string>
		<string>up.top=down.top</string>
		<string>up.height=down.height</string>
		<string>tips.left=title.left</string>
	</array>
